

{
  "masterPicture": "src/assets/favicon.svg",
	"iconsPath": "/",
	"design": {
		"ios": {
			"pictureAspect": "backgroundAndMargin",
			"backgroundColor": "#f7f7f8",
			"margin": "14%",
			"assets": {
				"ios6AndPriorIcons": true,
				"ios7AndLaterIcons": true,
				"precomposedIcons": true,
				"declareOnlyDefaultIcon": true
			}
		},
		"desktopBrowser": {
			"design": "background",
			"backgroundColor": "#f7f7f8",
			"backgroundRadius": 0.45,
			"imageScale": 0.8
		},
		"windows": {
			"pictureAspect": "whiteSilhouette",
			"backgroundColor": "#da532c",
			"onConflict": "override",
			"assets": {
				"windows80Ie10Tile": true,
				"windows10Ie11EdgeTiles": {
					"small": true,
					"medium": true,
					"big": true,
					"rectangle": true
				}
			}
		},
		"androidChrome": {
			"pictureAspect": "backgroundAndMargin",
			"margin": "17%",
			"backgroundColor": "#f7f7f8",
			"themeColor": "#f7f7f8",
			"manifest": {
				"startUrl": "https://where2factors.com",
				"display": "standalone",
				"orientation": "notSet",
				"onConflict": "override",
				"declared": true
			},
			"assets": {
				"legacyIcon": true,
				"lowResolutionIcons": false
			}
		},
		"safariPinnedTab": {
			"pictureAspect": "silhouette",
			"themeColor": "#dd491b"
		}
	},
	"settings": {
		"scalingAlgorithm": "Mitchell",
		"errorOnImageTooSmall": false,
		"readmeFile": false,
		"htmlCodeFile": true,
		"usePathAsIs": false
	}
}

