{
  "name": "where2factors",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "test": "mocha",
    "start": "node ./bin/www",
    "server": "nodemon ./bin/www",
    "client": "cd client && npm run serve",
    "dev": "concurrently --names \"SERVER,CLIENT\" \"npm run server\" \"npm run client\"",
    "update": "node cli.js update",
    "coverage": "nyc npm run test --check-coverage",
    "report-coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov"
  },
  "dependencies": {
    "axios": "^0.20.0",
    "chai-http": "^4.3.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "debug": "^4.1.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-validator": "^6.6.1",
    "helmet": "^4.1.1",
    "mongoose": "^5.10.2",
    "morgan": "^1.10.0",
    "psl": "^1.8.0",
    "v-tooltip": "^2.0.3",
    "winston": "^3.3.3",
    "winston-daily-rotate-file": "^4.5.0",
    "yaml": "^1.10.0",
    "yargs": "^16.1.0"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "codecov": "^3.8.0",
    "concurrently": "^5.3.0",
    "eslint": "^7.9.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-mocha": "^8.0.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-security": "^1.4.0",
    "mocha": "^8.1.3",
    "nodemon": "^2.0.4",
    "nyc": "^15.1.0",
    "prettier": "^2.1.1",
    "sinon": "^9.1.0"
  }
}
